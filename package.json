{
  "name": "dose-web",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:web": "node postinstall-web",
    "postinstall:electron": "node postinstall",
    "start": "PORT=4200 npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:serve:dev": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve --tools",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@ngxs-labs/dispatch-decorator": "^2.1.0",
    "@ngxs/devtools-plugin": "^3.6.2",
    "@ngxs/store": "^3.6.2",
    "@types/katex": "^0.11.0",
    "@types/mathjs": "^6.0.5",
    "electron-builder": "^22.6.0",
    "electron-reload": "^1.5.0",
    "hammerjs": "^2.0.8",
    "http-proxy": "^1.18.1",
    "mathjs": "^7.0.0",
    "ng-katex": "^2.0.3",
    "ng2-nouislider": "^1.8.2",
    "ng2-trim-directive": "^2.3.4",
    "nouislider": "^14.5.0",
    "npm-run-all": "^4.1.5",
    "plotly.js": "^1.54.1",
    "plotly.js-dist": "^1.54.1",
    "postinstall": "^0.7.0",
    "prettier": "^2.0.5",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "wait-on": "^5.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@ngxs-labs/select-snapshot": "^2.0.1",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-plotly.js": "^2.0.0",
    "codelyzer": "^5.1.2",
    "electron": "^18.3.7",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ngx-color-picker": "^9.1.0",
    "ngx-pipes": "^2.7.3",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
